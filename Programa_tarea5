#include <stdio.h>
 
int main(){
	
	unsigned short int est_num;
	
	printf("Estado disponibles\n");
	printf("\t1 para mostrar el nombre\n");
	printf("\t16 para mostrar los apellidos\n");
	printf("\t17 para mostrar el nombre completo\n");
	
	printf("\nIntroduzca el numero del estado que desea saber:");
	scanf("%i",&est_num);
	
	if(est_num==17){
		printf("\n\tMartinez Reyes Jose Alejandro\n");
	}
	else if(est_num==1){
		printf("\n\tJose Alejandro\n");
	}
	else if(est_num==16){
		printf("\n\tMartinez Reyes\n");
	}
	else{
		printf("\n\tOpcion invalida\n");
	}
	getch();
	return 0;	
}
